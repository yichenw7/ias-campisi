    "piwik-react-router": "^0.12.1",
    "prop-types": "^15.6.2",
    "rcc-calendar": "^1.0.9",
    "react-router-bundle": "^1.0.0",
    "ss-web-start": "^1.0.5",
    "store": "^2.0.12",
    "uuid": "^3.3.2",
    "ws": "^6.1.0"
  },
