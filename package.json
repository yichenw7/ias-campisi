    "@babel/helper-simple-access": "7.0.0-beta.31",
    "@babel/template": "7.0.0-beta.31",
    "@babel/types": "7.0.0-beta.31",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
